{
  "version": "1.0.0",
  "private": true,
  "name": "addi",
  "scripts": {
    "test": "jest --config .internals/tests/jest.config.js --coverage",
    "start": "react-app-rewired start",
    "build": "yarn build:prd & yarn build:non-prd",
    "build:prd": "cross-env REACT_APP_PRD=true REACT_APP_OUTPUT_PATH=build/prd react-app-rewired build",
    "build:non-prd": "cross-env REACT_APP_PRD=false REACT_APP_OUTPUT_PATH=build/non-prd react-app-rewired build --profile",
    "lint": "eslint . --ext .js,.ts,.tsx,.jsx",
    "storybook": "start-storybook -c .internals/storybook -s ./public -p 3003",
    "storybook:build": "build-storybook -c .internals/storybook -o .internals/storybook/out"
  },
  "prettier": "./.internals/.prettierrc.js",
  "babel": {
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.ts": "eslint --fix",
    "*.tsx": "eslint --fix"
  },
  "eslintConfig": {
    "extends": [
      "./.internals/.eslintrc.json"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "config-overrides-path": ".internals/config.js",
  "dependencies": {
    "@hot-loader/react-dom": "^17.0.2",
    "ky": "^0.28.7",
    "ky-universal": "^0.10.0",
    "react": "^17.0.2",
    "react-display-name": "^0.2.5",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.2.1"
  },
  "devDependencies": {
    "@sheerun/mutationobserver-shim": "^0.3.3",
    "@storybook/addon-docs": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.18",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.16",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.5.0",
    "cross-env": "^7.0.3",
    "css-loader": "5.2.6",
    "customize-cra": "^1.0.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.0",
    "jest-localstorage-mock": "^2.4.18",
    "lint-staged": "^12.3.3",
    "miragejs": "^0.1.43",
    "prettier": "^2.5.1",
    "react-app-rewired": "^2.1.11",
    "react-hot-loader": "^4.13.0",
    "react-scripts": "^5.0.0",
    "sass": "^1.49.7",
    "sass-loader": "10.1.1",
    "style-loader": "2.0.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
